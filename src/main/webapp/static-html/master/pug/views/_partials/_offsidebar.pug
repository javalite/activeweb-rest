// START Off Sidebar (right)
nav
    div(role='tabpanel')
        // Nav tabs
        ul.nav.nav-tabs.nav-justified(role='tablist')
            li.nav-item(role='presentation')
                a.nav-link.active(href='#app-settings', aria-controls='app-settings', role='tab', data-toggle='tab')
                    em.icon-equalizer.fa-lg
            li.nav-item(role='presentation')
                a.nav-link(href='#app-chat', aria-controls='app-chat', role='tab', data-toggle='tab')
                    em.icon-user.fa-lg
        // Tab panes
        .tab-content
            #app-settings.tab-pane.fade.active.show(role='tabpanel')
                include _settings
            #app-chat.tab-pane.fade(role='tabpanel')
                include _chat

// END Off Sidebar (right)
